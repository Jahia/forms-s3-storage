<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <forms-s3-storage j:dependencies="default forms-core"
                        j:modulePriority="0"
                        j:moduleType="module"
                        j:title="Forms S3 storage"
                        jcr:primaryType="jnt:module">
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder">
               <form-factory-actions jcr:primaryType="jnt:contentFolder">
                  <save-binary-to-s3 jcr:primaryType="fcnt:saveBinaryToS3Action">
                     <j:translation_en jcr:language="en"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Save Binary To S3"/>
                     <actionname jcr:primaryType="fcnt:definitionOptions" jsonValue="savebinarytos3"/>
                  </save-binary-to-s3>
               </form-factory-actions>
            </contents>
         </templates>
      </forms-s3-storage>
   </modules>
</content>
